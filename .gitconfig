[user]
	name = David Laribee
	email = laribee@gmail.com
[color]
	branch = auto
	diff = auto
	status = auto
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold
[color "status"]
  added = yellow
  changed = green
  untracked = cyan
[merge]
	tool = opendiff
[core]
	legacyheaders = false
[repack]
	usedeltabaseoffset = true
[alias]
	ci = commit
	co = checkout
	st = status
  ch = cherry -v origin
  br = branch -a
  mine = log --author=David
[github]
	user = laribee
	token = 
[diff "ruby"]
	funcname = ^ *\\(\\(class\\|module\\|def\\) .*\\)
[push]
	default = matching
[branch "master"]
	remote = origin
	merge = refs/heads/master
